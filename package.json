		"prettier": "^3.6.2",
		"prettier-plugin-svelte": "^3.4.0",
		"svelte": "^5.38.1",
		"svelte-check": "^4.3.1",
		"svelte-eslint-parser": "^1.3.1",
		"typescript": "^5.9.2",
