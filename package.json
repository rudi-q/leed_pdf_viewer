		"@tauri-apps/plugin-updater": "~2",
		"@types/markdown-it": "^14.1.2",
		"@vercel/analytics": "^1.5.0",
		"autoprefixer": "^10.4.21",
		"jspdf": "^3.0.2",
		"lucide-svelte": "^0.544.0",
		"markdown-it": "^14.1.0",
		"pdf-lib": "^1.17.1",
		"pdfjs-dist": "5.4.54",
